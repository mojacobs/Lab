@model List<RideSharePlus.Models.Ride>

<h2><b>Students starting in the same town:</b></h2>

<div id="sametown">
        @if (Model.Count == 0)
        {
            <p>No Results!</p>
        }
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.StudentEmail</li>
            }
        </ul>
</div>